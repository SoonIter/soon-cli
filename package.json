{
  "name": "soon-cli",
  "version": "1.0.0",
  "packageManager": "pnpm@7.5.0",
  "description": "自用的库模版",
  "author": {
    "name": "SoonIter",
    "email": "1730214056@qq.com"
  },
  "license": "MIT",
  "repository": "https://github.com/SoonIter/soon-cli",
  "bugs": {
    "url": "https://github.com/SoonIter/soon-cli/issues"
  },
  "keywords": [
    "cli"
  ],
  "bin": {
    "soon": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev":"tsup src/index.ts --watch --onSuccess \"npx soon create hello-app\"",
    "build": "tsup ./src/index.ts",
    "release": "nr prerelease && nr build && npm publish",
    "prerelease": "nr lint:fix && nr test:once",
    "test": "vitest",
    "test:once": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.*",
    "lint:fix": "eslint src/**/*.* --fix"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.3",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.4",
    "@vitest/ui": "^0.15.1",
    "c8": "^7.11.3",
    "eslint": "^8.18.0",
    "lint-staged": "13.0.3",
    "simple-git-hooks": "^2.8.0",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.18.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "ora": "^5.4.1",
    "pacote": "^12.0.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}
